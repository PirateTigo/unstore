{
  "name": "@unstore/backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "rimraf ./dist && tsc --build ./tsconfig.json",
    "dev": "ts-node-dev --watch ./.env --rs --exit-child ./src/index.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts .",
    "pgc": "prisma generate",
    "pmd": "prisma migrate dev",
    "prepare": "pnpm run pgc",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "start": "node ./dist/index.js",
    "types": "tsc --noEmit --project ./tsconfig.json"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@trpc/server": "^10.45.2",
    "cors": "^2.8.5",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "prisma": "^6.3.1",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0"
  }
}
